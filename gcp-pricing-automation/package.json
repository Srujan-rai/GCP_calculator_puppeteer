{
  "name": "gcp-pricing-automation",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run start:containers\" \"next dev\"",
    "start:containers": "docker compose start",
    "stop:containers": "docker compose stop",
    "rebuild:containers": "docker compose up --build --detach",
    "clean:containers": "docker compose down --volumes --remove-orphans",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "express": "^4.21.2",
    "google-auth-library": "^9.15.1",
    "google-spreadsheet": "^3.3.0",
    "googleapis": "^148.0.0",
    "next": "15.2.4",
    "node-fetch": "^2.7.0",
    "puppeteer": "^24.4.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.0",
    "eslint": "^9",
    "eslint-config-next": "15.2.4",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
